<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000">

    <ImageView
        android:id="@+id/search_bar"
        android:layout_width="337dp"
        android:layout_height="51dp"
        android:layout_marginTop="35dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/search_bar"
        tools:layout_editor_absoluteX="13dp" />

    <TextView
        android:id="@+id/search_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:fontFamily="@font/pretendard_regular"
        android:text="Search.."
        android:textColor="#a6a6a6"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="70dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/search_bar_icon"
        tools:layout_editor_absoluteX="30dp" />

    <TextView
        android:id="@+id/history_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:fontFamily="@font/pretendard_bold"
        android:text="History"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintTop_toBottomOf="@+id/search_bar"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/history_text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/pretendard_bold"
        android:text="#interior"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/history_title"
        tools:layout_editor_absoluteX="16dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="25dp"
        android:layout_height="22dp"
        app:srcCompat="@drawable/search_go"
        tools:layout_editor_absoluteX="375dp"
        tools:layout_editor_absoluteY="167dp" />

    <ImageView
        android:id="@+id/search_school"
        android:layout_width="40dp"
        android:layout_height="42dp"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/search_school"
        tools:layout_editor_absoluteX="360dp" />

</androidx.constraintlayout.widget.ConstraintLayout>